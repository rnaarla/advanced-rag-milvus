{
  "title": "RAG Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Retrieve Latency (ms)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(rag_retrieve_latency_ms_bucket[5m])) by (le))", "legendFormat": "P95" },
        { "expr": "histogram_quantile(0.50, sum(rate(rag_retrieve_latency_ms_bucket[5m])) by (le))", "legendFormat": "P50" }
      ]
    },
    {
      "type": "stat",
      "title": "Requests (5m)",
      "targets": [
        { "expr": "sum(increase(rag_api_requests_total[5m]))", "legendFormat": "reqs" }
      ]
    }
  ],
  "schemaVersion": 37,
  "version": 1
}


